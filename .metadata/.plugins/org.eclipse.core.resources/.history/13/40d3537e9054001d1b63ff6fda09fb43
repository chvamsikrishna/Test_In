package basics;

public class Abstract_2 extends Abstract_1 {

	int physicsMarks;
	int chemistryMarks;
	int mathsMarks;
	static int students_1;
	
	public Abstract_2(String name, int standard) {
		super(name, standard);
	}


	@Override
	int getPercantage() {
	
		int value1 = (physicsMarks+chemistryMarks+mathsMarks)*3;
		value1/=100;
		return value1;
	}

}


