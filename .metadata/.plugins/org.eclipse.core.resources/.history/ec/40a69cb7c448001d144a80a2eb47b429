package basics;

public class StaticClass {
	int num2= 10;
	static int num1 =10;
	
	public void marks()
	{
		num1 += 10;
		num2+=10;
	
	}
	
	

	public static void main(String[] args) {

	
	
	 StaticClass sc = new StaticClass();
     sc.marks();
     System.out.println(num1+" "+ sc.num2);
	 
	 
	 
	}

}
